{
    "version": 6,

    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 19,
        "patch": 0
    },

    "configurePresets": [
    {
        "name": "debug-x64",
        "architecture": {
            "value": "x64",
            "strategy": "external"
        },
        "cacheVariables": {
            "CMAKE_BUILD_TYPE": "Debug"
        }
    },

    {
        "name": "gcc-base",
        "generator": "MinGW Makefiles"
    },

    {
        "name": "makefile-gcc-debug",
         "inherits": [
            "gcc-base",
            "debug-x64"
        ] 
    },
    
    {
        "name": "mvsc-base",
        "generator": "Visual Studio 17 2022"
    },

    {
        "name": "vs2022-mvsc-debug",
        "inherits": [
            "mvsc-base",
            "debug-x64"
        ]
    }

    ]
}